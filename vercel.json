{
  "version": 3,
  "routes": [
    {
      "src": "/auth",
      "dest": "/api/auth.js",
      "methods": ["GET"]
    },
    {
      "src": "/repo",
      "dest": "/api/repo.js",
      "methods": ["GET"]
    },
    {
      "src": "/branch",
      "dest": "/api/branch.js",
      "methods": ["GET"]
    },
    {
      "src": "/save",
      "dest": "/api/save.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/not-found.js"
    }
  ]
}
